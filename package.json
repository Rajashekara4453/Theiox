  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "build.prod": "ng build --prod && find dist/elmeasure_ui \\( -name documentation -o -name assets \\) -prune -o \\( -name '*.css' -o -name '*.js' \\) -exec gzip --force --keep --verbose {} + && yarn run post.build",
